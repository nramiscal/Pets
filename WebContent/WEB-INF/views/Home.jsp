<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Home Page</title>
</head>
<body>
    <h1>Welcome to Pets!</h1>
    <h2>Create a Dog</h2>
    <form class="" action="dog" method="get">
        <input type="text" name="name" placeholder="Name"><br>
        <input type="text" name="breed" placeholder="Breed"><br>
        <input type="number" name="weight" placeholder="Weight"><br>
        <input type="submit" name="" value="Create Dog">
    </form>

    <h2>Create a Cat</h2>
    <form class="" action="cat" method="get">
        <input type="text" name="name" placeholder="Name"><br>
        <input type="text" name="breed" placeholder="Breed"><br>
        <input type="number" name="weight" placeholder="Weight"><br>
        <input type="submit" name="" value="Create Cat">
    </form>


</body>
</html>